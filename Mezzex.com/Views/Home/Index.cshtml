	<script>
		$(document).ready(function () {

			document.querySelector('.default-btn').addEventListener('click', function (e) {
				e.preventDefault();  // Prevent the default jump behavior
				document.querySelector('#services').scrollIntoView({
					behavior: 'smooth'
				});
			});

			$('.truncate-content').each(function () {
				var content = $(this).text();
				var limit = 277;

				if (content.length > limit) {
					var truncated = content.substring(0, limit) + '...';
					$(this).text(truncated);
				}
			});
			// Function to check if element is in viewport
			function isInViewport(element) {
				var elementTop = $(element).offset().top;
				var elementBottom = elementTop + $(element).outerHeight();
				var viewportTop = $(window).scrollTop();
				var viewportBottom = viewportTop + $(window).height();
				return elementBottom > viewportTop && elementTop < viewportBottom;
			}

			// Counter animation function
			function animateCounters() {
				$('.counter-number').each(function () {
					var $this = $(this);
					var countTo = $this.attr('data-target');

					// Animate counter if it hasn't been triggered yet
					if (!$this.hasClass('counted') && isInViewport($this)) {
						$this.addClass('counted');
						$({ countNum: $this.text() }).animate(
							{ countNum: countTo },
							{
								duration: 2000,
								easing: 'swing',
								step: function () {
									$this.text(Math.floor(this.countNum));
								},
								complete: function () {
									$this.text(this.countNum);
								}
							}
						);
					}
				});
			}

			// Initial counter animation on page load
			animateCounters();

			// Trigger counter animation on scroll
			$(window).on('scroll', function () {
				animateCounters();
			});
			// Hide all tabs content except the first one
			$(".tab_content .tabs_item").hide();
			$(".tab_content .tabs_item:first").show();

			// Add active class to the first tab
			$(".tabs li:first").addClass("active");

			// When a tab is clicked
			$(".tabs li a").click(function (e) {
				e.preventDefault();

				// Remove active class from all tabs and add to the clicked tab
				$(".tabs li").removeClass("active");
				$(this).parent().addClass("active");

				// Hide all tab content
				$(".tab_content .tabs_item").hide();

				// Show the corresponding tab content
				var activeTab = $(this).attr("href");
				$(activeTab).fadeIn();
			});
		});
	</script>
